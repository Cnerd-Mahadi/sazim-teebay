{
	"name": "teebay-api",
	"version": "1.0.0",
	"description": "teebay backend services and api",
	"scripts": {
		"dev": "nodemon",
		"build": "rm -rf dist && tsc && tsc-alias",
		"lint": "eslint . - ext .ts",
		"dotenv": "dotenv -e .env.local --",
		"prisma:seed": "yarn dotenv ts-node -r tsconfig-paths/register ./prisma/seed.ts",
		"prisma:migrate": "yarn dotenv prisma migrate dev",
		"prisma:studio": "yarn dotenv prisma studio",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [
		"teebay",
		"sazim",
		"api"
	],
	"author": "cnerd",
	"license": "ISC",
	"dependencies": {
		"@prisma/client": "^5.14.0",
		"@trpc/server": "^11.0.0-rc.433",
		"bcrypt": "^5.1.1",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"jsonwebtoken": "^9.0.2",
		"prisma": "^5.14.0",
		"superjson": "^1.13.3",
		"tsc-alias": "^1.8.10",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@eslint/js": "^9.10.0",
		"@types/bcrypt": "^5.0.2",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/node": "^22.5.5",
		"eslint": "^9.10.0",
		"globals": "^15.9.0",
		"nodemon": "^3.1.2",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.6.2",
		"typescript-eslint": "^8.6.0"
	}
}
